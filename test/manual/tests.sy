int add_int(int a, int b)
{
    int c;
    c = a + b;
    return c;
}
int main()
{
    int i = 0;
    int j = 0;
    int b;
    while(i < 10 && j < 1){
        if(i < 5){
            b = add_int(i,j);
        } else{
            b = 0;
        }
        i = i + 2;
        j = j + 1;
    }
    return 0;
}